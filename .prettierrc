{
  "trailingComma": "none",
  "tabWidth": 2,
  "useTabs": true,
  "singleQuote": false,
  "jsxSingleQuote": false,
  "arrowParens": "avoid",
  "semi": true,
  "importOrder": [
    "<THIRD_PARTY_MODULES>",
    "^@/components/(.*)$",
    "^@/hooks/(.*)$",
    "^@/shared/(.*)$",
    "^@/services/(.*)$",
    "^@/assets/(.*)$",
    "^@/utils/(.*)$",
    "^@/config/(.*)$",
    "^@/providers/(.*)$",
    "^../(.*)",
    "^./(.*)"
  ],
  "importOrderSeparation": true,
  "importOrderSortSeparation": true
}